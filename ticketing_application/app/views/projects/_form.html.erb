<%= render "shared/errors", obj: @project%>

<%= form_for @project do |f| %>
  <div class="input-group">
    <%= f.label :name%>
    <%= f.text_field :name%>
  </div>
  <div class="input-group">
    <%= f.label :description%>
    <%= f.text_area :description%>
  </div>
  <div class="input-group">
    <%= f.submit class: "btn btn-success"%>
    <%= link_to "Back", :back, class: "btn" %>
  </div>
<% end %>